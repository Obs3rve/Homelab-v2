# terraform.tfvars

pi1_ip            = "192.168.1.3"
pi2_ip            = "192.168.1.4"
pi1_user          = "pi1"      # SSH username for Pi1
pi2_user          = "pi2"      # SSH username for Pi2
ssh_private_key_path = "~/.ssh/id_rsa"  # path to your private SSH key
k3s_token          = "your-k3s-token"  # k3s token from master node
